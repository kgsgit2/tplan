# Copy this file to .env.local and fill in your actual values

# Kakao API Key - Get from https://developers.kakao.com/
NEXT_PUBLIC_KAKAO_API_KEY=your_kakao_api_key_here

# Supabase Configuration - Get from https://supabase.com/dashboard
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Toss Payments - Get from https://developers.tosspayments.com/
NEXT_PUBLIC_TOSS_CLIENT_KEY=your_toss_client_key_here

# Development Admin Credentials
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=secure_password_here

# Database Connection String
DATABASE_URL=your_database_connection_string